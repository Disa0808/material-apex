        <div class="ma-wizard-step-wrap">
            <span class="ma-wizard-step-marker complete green lighten-2"><i class="material-icons">done</i></span>
            <span class="ma-wizard-step-marker not-complete"><i class="material-icons">#ICON_CSS_CLASSES#</i></span>
            <span class="ma-wizard-step-label">#TEXT#</span>
        </div>
    </li>
    {{/list_template_noncurrent}}
    {{#item_template_curr_w_child}}
    {{/item_template_curr_w_child}}
    {{#item_template_noncurr_w_child}}
    {{/item_template_noncurr_w_child}}
    {{#between_items}}
    {{/between_items}}

    {{#before_sub_list}}
    {{/before_sub_list}}
        {{#sub_list_item_current}}
        {{/sub_list_item_current}}
        {{#sub_template_curr_w_child}}{{/sub_template_curr_w_child}}
        {{#sub_list_item_noncurrent}}
        {{/sub_list_item_noncurrent}}
        {{#sub_template_noncurr_w_child}}{{/sub_template_noncurr_w_child}}
        {{#between_sub_list_items}}{{/between_sub_list_items}}
    {{#after_sub_list}}
